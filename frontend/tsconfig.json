{
    "compilerOptions": {
      "sourceMap": true,
      "target": "ES2022",
      // "module": "CommonJS",
      "module": "ES2022",
      "outDir": "../static",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "incremental": true
    },
    "include": ["./src/**/*.ts"],
    "exclude": ["./node_modules"]
}
  